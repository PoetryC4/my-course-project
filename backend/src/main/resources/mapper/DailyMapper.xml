<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.companypowernode.dao.mapper.DailyMapper">

    <select id="getdaily" resultType="com.companypowernode.dao.entity.Daily">
        select DATE_FORMAT(trade_date,'%Y/%m/%d') as trade_date,open,close,low,high from daily where ts_code=${tsCode}
    </select>
</mapper>
